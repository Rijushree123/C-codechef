#include <iostream>
#include<math.h>
using namespace std;
#define ll long long
int main()
{
    int T;
    cin>>T;
    while(T--)
    {
        int N;
        cin>>N;
        if(N==0)
        {
            cout<<"1"<<endl;
        }
        else if(N==1)
        {
            cout<<"2"<<endl;
        }
        else if(N==2)
        {
            cout<<"2"<<endl;
        }
        else
        {
            ll ans=1;
            while(2*ans<=N)
            {
                ans*=2;
            }
            if(ans==N)
            {
                cout<<N<<endl;
            }
            else if(N==(2*ans-1))
            {
                cout<<N+1<<endl;
            }
            else
            {
                cout<<ans<<endl;
            }
        }
    }
}
#Rijushree Guha
